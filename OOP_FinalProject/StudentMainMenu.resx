  <data name="$this.Text" xml:space="preserve">
    <value>SELECT DISTINCT
    Courses.COURSE_ID AS CourseCode,
    Courses.COURSE_NAME AS CourseName,
    Department.DEPARTMENT_NAME AS DepartmentName,
    CONCAT(Professor.LAST_NAME, ', ', Professor.FIRST_NAME) AS ProfessorName,
    Classes.TERM AS Semester,
    FinalGrades.MIDTERM_PERCENTAGE,
    FinalGrades.MIDTERM_GWA,
    FinalGrades.FINALS_PERCENTAGE,
    FinalGrades.FINALS_GWA,
    FinalGrades.SEMESTRAL_PERCENTAGE,
    FinalGrades.SEMESTRAL_GWA
FROM
    STUDENT
INNER JOIN LOGIN ON STUDENT.USER_ID = LOGIN.USER_ID
INNER JOIN ENROLLMENTS ON STUDENT.Student_ID = ENROLLMENTS.Student_ID
INNER JOIN CLASSES ON ENROLLMENTS.CLASS_ID = CLASSES.CLASS_ID
INNER JOIN COURSES ON CLASSES.SECTION_ID IN (SELECT SECTION_ID FROM SECTIONS WHERE COURSES.COURSE_ID = COURSES.COURSE_ID)
INNER JOIN DEPARTMENT ON COURSES.DEPARTMENT_ID = DEPARTMENT.DEPARTMENT_ID
INNER JOIN PROFESSOR ON CLASSES.PROFESSOR_ID = PROFESSOR.PROFESSOR_ID
INNER JOIN FINAL_GRADES ON FINAL_GRADES.CLASS_ID = CLASSES.CLASS_ID
WHERE
    LOGIN.USER_ID = @userID;
</value>
